<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
	<PackageVersion>0.0.1-beta</PackageVersion>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<Title>BlazorBeerCSS</Title>
	<PackageProjectUrl>https://kristianfriis.github.io/BlazorBeerCss/</PackageProjectUrl>
  </PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.16" />
	<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.16" />
  </ItemGroup>

</Project>
